# GermanCodeZero-Agent Environment Variables
# Copy this file to .env and adjust the values

# PostgreSQL
POSTGRES_PASSWORD=secure_gcz_password_2024

# n8n
N8N_ENCRYPTION_KEY=gcz_n8n_encryption_key_2024

# Ollama Settings (for native Windows installation)
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODELS_PATH=C:/Users/%USERNAME%/.ollama/models

# DirectML/Vulkan Settings for Windows 11
USE_DIRECTML=true
USE_VULKAN=false
GPU_LAYERS=-1  # -1 for all layers on GPU

# Model Settings
DEFAULT_CODE_MODEL=deepseek-coder:33b
DEFAULT_REASONING_MODEL=llama3:70b
DEFAULT_LIGHT_MODEL=mistral:7b

# API Keys (if using cloud services as fallback)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Agent Settings
AGENT_REGISTRY_PATH=./data/registry.db
AGENT_WORKSPACE_PATH=./workspace
AGENT_LOG_LEVEL=INFO

# Redis Settings
REDIS_URL=redis://localhost:6379
REDIS_MAX_CONNECTIONS=10

# Qdrant Settings
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION=agent_embeddings

# n8n Workflow Settings
N8N_WEBHOOK_URL=http://localhost:5678/webhook

# Monitoring
ENABLE_MONITORING=true
METRICS_PORT=9090